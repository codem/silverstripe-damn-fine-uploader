{
    "name": "codem/silverstripe-damn-fine-uploader",
    "description": "An HTML5 upload field for Silverstripe 4, including editable form fields for silverstripe/userforms",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "uploader",
        "uploads",
        "uppy",
        "html5",
        "file upload"
    ],
    "homepage": "https://github.com/codem/silverstripe-damn-fine-uploader",
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "James Ellis",
        "homepage": "https://github.com/codem/"
    }],
    "require": {
        "silverstripe/framework": "~4.0",
        "paragonie/random_compat": ">=2.0",
        "silverstripe/userforms": "~5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5",
        "codeception/codeception": "^2.5",
        "vlucas/phpdotenv" : "^3",
        "symbiote/silverstripe-test-assist": "^4.0"
    },
    "extra": {
        "expose": [
                "client/dist/js",
                "client/dist/styles",
                "client/dist/assets"
        ]
    }
}
