{
  "name": "codem-silverstripe-damn-fine-uploader",
  "version": "1.0.0",
  "description": "A Damn Fine Uploader for Silverstripe 4",
  "license": "MIT",
  "author": "James Ellis",
  "engines": {
    "node": ">= 6.x"
  },
  "scripts": {
    "build": "yarn && NODE_ENV=production webpack -p --bail --progress",
    "dev": "NODE_ENV=development webpack --progress",
    "watch": "yarn && NODE_ENV=development webpack --watch --progress",
    "css": "yarn && WEBPACK_CHILD=css webpack -p --bail --progress",
    "lock": "npm-shrinkwrap --dev",
    "lint": "eslint client/src && sass-lint -v client/src"
  },
  "devDependencies": {
    "@babel/core": "^7.11",
    "@babel/plugin-transform-arrow-functions": "^7.10",
    "@babel/plugin-transform-runtime": "^7.11",
    "@babel/polyfill": "^7.10",
    "@babel/preset-env": "^7.11",
    "babel-loader": "^8.1",
    "copy-webpack-plugin": "^6",
    "css-loader": "^4.2",
    "file-loader": "^6",
    "mini-css-extract-plugin": "^0.11",
    "style-loader": "^1.2",
    "uglifyjs-webpack-plugin": "^2.2",
    "webpack": "^4.44",
    "webpack-cli": "^3.3"
  },
  "dependencies": {
    "@uppy/core": "^0.30.3",
    "@uppy/dashboard": "^0.30.3",
    "@uppy/drag-drop": "^0.30.3",
    "@uppy/xhr-upload": "^0.30.3",
    "fine-uploader": "^5.16.2"
  }
}
